{
  "storybook.vue": {
    "prefix": ["storybook.vue"],
    "body": [
      "import { Story, Meta } from \"@storybook/vue\";",
      "import SomeComponent from \"~/components/path/to/Some.vue\"",
      "",
      "export default {",
      "  title: \"path/to/Some\",",
      "  component: SomeComponent,",
      "  argTypes: {",
      "    var1: {",
      "      control: { type: \"object\" },",
      "    },",
      "    var2: {",
      "      control: { type: \"select\" },",
      "      options: [\"button\", \"submit\"],",
      "    },",
      "    var3: {",
      "      control: { type: \"boolean\" },",
      "    },",
      "    var4: {",
      "      control: { type: \"text\" },",
      "    },",
      "    onClick: {",
      "      action: \"clicked\",",
      "    },",
      "  },",
      "} as Meta;",
      "",
      "const Template: Story = (args) => ({",
      "  components: { SomeComponent },",
      "  setup() {",
      "    return { args };",
      "  },",
      "  template: `<SomeComponent v-bind=\"args\" @click=\"args.onClick\" />`,",
      "});",
      "",
      "export const Default = Template.bind({});",
      "Default.args = {",
      "  var1: {},",
      "  var2: \"button\",",
      "  var3: false,",
      "  var5: \"hoge\",",
      "};"
    ],
    "description": "Test code by Storybook in Vue.js."
  },
  "jest.nuxt.plugin": {
    "prefix": ["jest.nuxt.plugin"],
    "body": [
      "import { Context } from \"@nuxt/types\";",
      "import samplePlugin from \"~/plugins/sample\";",
      "",
      "const mockContext: Context = {} as Context;",
      "",
      "describe(\"components/path/to/Component\", () => {",
      "  samplePlugin(",
      "    mockContext,",
      "    (key, value) => ((mockContext as any)[\"\\$\" + key] = value)",
      "  );",
      "",
      "  test(\"test\", () => {",
      "  });",
      "});"
    ],
    "description": "Test code by jest in Nuxt.js plugins."
  }
}
